{
  "name": "@adxp/ws-relay",
  "version": "0.1.7",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "default": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./server": {
      "default": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc -w & nodemon -q -w dist dist/server/index.js",
    "build": "tsc",
    "test": "ava -v test/*.ts"
  },
  "dependencies": {
    "express": "^4.18.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "ava": "^4.3.0",
    "nodemon": "^2.0.16",
    "typescript": "^4.7.2"
  }
}
