{
  "name": "channel-relay",
  "version": "0.1.7",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    } ,
    "./client": {
      "default": "./dist/client.js",
      "types": "./dist/client.d.ts"
    } ,
    "./server": {
      "default": "./dist/server.js",
      "types": "./dist/server.d.ts"
    } 
  },
  "typesVersions": {
    ">=4.1": { 
        "*": {
            "index": ["./dist/index.d.ts"],
            "client": ["./dist/client.d.ts"]
        }
    }
  },
  "scripts": {
    "dev": "tsc -w & nodemon -q -w dist dist/server/index.js",
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  }
}
