{
  "name": "@adxp/dev-env",
  "version": "1.0.0",
  "description": "An example monorepo package with testing/esbuild",
  "main": "src/index.ts",
  "scripts": {
    "build": "node ./build.js",
    "start": "node --loader ts-node/esm ./src/index.ts",
    "test": "jest",
    "prettier": "prettier --check src/",
    "prettier:fix": "prettier --write src/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "verify": "run-p prettier lint",
    "verify:fix": "yarn prettier:fix && yarn lint:fix"
  },
  "author": "Rahul Tarak",
  "license": "MIT",
  "dependencies": {
    "@adxp/auth-lobby": "*",
    "@adxp/common": "*",
    "@adxp/crypto": "*",
    "@adxp/did-sdk": "*",
    "@adxp/example-app": "*",
    "@adxp/server": "*",
    "@adxp/ws-relay": "*",
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "get-port": "^6.1.2",
    "level": "^8.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13"
  }
}
